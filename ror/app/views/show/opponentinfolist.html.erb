<table class="table table-hover fixtable" >
	<tbody>
	<% @infos.each do |info| %>
		<tr>
			<td width="20%"><%= info.source_name %></td>
				<td width="70%" title="<%= info.title %>"><a href="/show/single_opponent/<%= info.id %>"><%= info.title %></a></td>
			
		</tr>
	<% end %>
	</tbody>
</table>
<div class="pagination pagination-right margin0">
	<ul>
		<li><a href="javascript:;" class="prev<%= params[:opponent_keyword_id] %>">上一页</a></li>
		<li><a href="javascript:;" class="next<%= params[:opponent_keyword_id] %>">下一页</a></li>
	</ul>
</div>